<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.m.data.mapper.StudentDao">


    <!--自定义sql-->
    <select id="getBYPk" resultType="com.m.data.domain.StudentVo">
        SELECT id, name, cid, age, email
        FROM student
        WHERE id = #{id}
    </select>

</mapper>
